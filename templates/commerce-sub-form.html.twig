{#
/**
 * @file
 * Two column template for the sub add/edit form.
 *
 * Available variables:
 * - form: The form.
 *
 * @ingroup themeable
 */
#}
<div class="layout-sub-form clearfix">
  <div class="layout-region layout-region-sub-main">
    {{ form|without('advanced', 'actions') }}
  </div>
  <div class="layout-region layout-region-sub-secondary">
    {{ form.advanced }}
  </div>
  <div class="layout-region layout-region-sub-footer">
    {{ form.actions }}
  </div>
</div>
